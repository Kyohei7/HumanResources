<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_hr"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_hr_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OnBoard3Activity"></activity>
        <activity android:name=".OnBoard2Activity" />
        <activity android:name=".OnBoard1Activity" />
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".LoginActivity"
            android:label="Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>